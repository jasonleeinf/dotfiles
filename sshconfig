# this allows `ssh devfair` or `et devfair:8080 -t="1234:22,8888:8888,9090:9090" -jport=8080`
Host *-fairjmp*
    IdentityFile ~/.ssh/fb_id_rsa

Host devfair*
    IdentityFile ~/.ssh/fb_id_rsa
    ProxyJump frc-fairjmp01

